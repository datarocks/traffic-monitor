{
    "name": "buster-nodered",
    "description": "Traffic monitoring solution node-red flow",
    "version": "0.0.1",
    "dependencies": {
        "node-red-contrib-aedes": "0.12.0",
        "node-red-node-serialport": "2.0.2",
        "node-red-dashboard": "3.6.2",
        "node-red-contrib-dashboard-bar-chart-data": "0.9.93",
        "node-red-node-smooth": "0.1.2",
        "node-red-contrib-influxdb": "0.7.0",
        "node-red-contrib-mongodb4": "2.5.2"
    },
    "node-red": {
        "settings": {
            "flowFile": "flows.json",
            "credentialsFile": "flows_cred.json"
        }
    },
    "scripts": {
        "start": "node $NODE_OPTIONS node_modules/node-red/red.js $FLOWS"
    }
}